{
  "name": "@shared-libs/ui",
  "projectType": "library",
  "sourceRoot": "packages/ui/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/packages/ui/dist"],
      "options": {
        "command": "pnpm run build",
        "cwd": "packages/ui"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run test",
        "cwd": "packages/ui"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run lint",
        "cwd": "packages/ui"
      }
    }
  }
}
